<%- include("partials/header.ejs") %>

<div class="titleBlock">
  <img class="logo" src="https://cdn.onlinewebfonts.com/svg/img_81173.svg" alt="Dashboard icon">
  <h1>DashBoardSG</h1>
</div>

<div class="row">
  <% Object.keys(selectedDash).forEach(key => { %>
    <div class="card col-xs-12 col-sm-6 col-md-4 col-lg-3">
      <strong><%= selectedDash[key].title %></strong>
      <div class="value"><%= selectedDash[key].value %></div>
      <div class="unit"><%= selectedDash[key].unit %></div>
    </div>
  <% }) %>
</div>
<%- include("partials/footer.ejs") %>
